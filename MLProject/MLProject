name: sml-trio-anggoro-CI

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      dataset: {type: string, default: "Telco-Customer-Churn_preprocessing.csv"}
      penalty: {type: string, default: "l2"}
      C: {type: float, default: 0.1}
      solver: {type: string, default: "liblinear"}
    command: "python modelling.py {dataset} {penalty} {C} {solver}"
